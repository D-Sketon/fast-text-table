{
  "name": "fast-text-table",
  "version": "0.1.0",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.cjs"
    },
    "./*": "./*"
  },
  "scripts": {
    "test": "vitest",
    "bench": "vitest bench",
    "build": "rollup -c"
  },
  "sideEffects": false,
  "author": "D-Sketon",
  "license": "MIT",
  "description": "Quickly generate borderless text tables with alignment",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/cli-color": "^2.0.6",
    "@types/text-table": "^0.2.5",
    "cli-color": "^2.0.4",
    "rollup": "^4.34.0",
    "text-table": "^0.2.0",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "vitest": "^3.0.4"
  }
}
